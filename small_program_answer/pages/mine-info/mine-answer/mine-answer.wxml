<view class="container">
    <view class="mine">
        <image class="mine-answer-icon mine-img" src="../../../images/icon/answer.png"></image>
        <text class="total">总计答对题目数：</text>
        <text class="num">{{Correct}}</text>
        <text class="ti">题</text>
    </view>
    <view class="mine" wx:for="{{HistoryList}}">
        <image class="mine-answer-icon mine-img" src="../../../images/icon/answer.png"></image>
        <text class="date">{{item.AnswerDate}}</text>
        <text class="total-answer">共计答题：</text>
        <text class="answer">{{item.Total}}题</text>
        <text class="answer-true">共答对：</text>
        <text class="true-total">{{item.CorrectNumber}}题</text>
    </view>
    
</view>